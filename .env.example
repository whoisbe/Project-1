# Typesense Configuration
TYPESENSE_HOST=localhost
TYPESENSE_PORT=8108
TYPESENSE_PROTOCOL=http
TYPESENSE_API_KEY=xyz

# OpenAI Configuration
OPENAI_API_KEY=sk-...
OPENAI_EMBED_MODEL=text-embedding-3-small

# Reranker Configuration
RERANK_PROVIDER=cohere
COHERE_API_KEY=...
COHERE_RERANK_MODEL=rerank-english-v3.0

# Local Docs Repository Configuration
# Absolute path to the Typesense docs repository on your filesystem
DOCS_REPO_PATH=/path/to/typesense-website/docs-site
# Base URL for constructing document URLs (should match the live site)
DOCS_BASE_URL=https://typesense.org/docs
# Source identifier for the crawled pages
DOCS_SOURCE_ID=typesense-docs
